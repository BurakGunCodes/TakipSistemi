@model TakipSistemi.Models.Maps.Location

<input type="submit" value="Post Et" id="burakbuton" />

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

<script type="text/javascript">

    var  coordinates = [];
    var id; // details basıldıgı zaman alacagı deger

    $("#burakbuton").click(function () {
        console.log("burak kardes");

                $.ajax({
                    type: "POST",
                    url: "/Map/GetCoordinate",
                    data: { y : id },
                    success: function (response) {
                        //alert("Hello:" +  response);
                        //array.push({"Lng:":response.Lng});
                       
                        if(response.length > 0){
                            coordinates = response;
                        }
                        console.log(coordinates);
                    },
                    failure: function (response) {
                        alert(response.responseText);
                    },
                    error: function (response) {
                        alert(response.responseText);
                   }
                });
            });

     

</script>






